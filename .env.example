# TilexCare Environment Configuration
# Copy this file to .env and fill in your values
# For Docker deployment: docker compose up --build -d

# ==================== DATABASE ====================
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=tilexcare

# ==================== AUTHENTICATION ====================
# IMPORTANT: Change this in production!
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# ==================== SERVER ====================
PORT=5000
NODE_ENV=development

# ==================== ZOOM VIDEO SDK (Optional) ====================
# Required for video consultations
# Get credentials from: https://marketplace.zoom.us/
ZOOM_SDK_KEY=
ZOOM_SDK_SECRET=
